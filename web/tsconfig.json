{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "esnext",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./src",
    "lib": ["dom", "esnext"],
    "esModuleInterop": true,
    "paths": {
      "@components/*": ["./components/*"],
      "@pages/*": ["./pages/*"]
    }
  },
  "exclude": ["node_modules"]
}
